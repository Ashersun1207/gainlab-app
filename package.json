  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "bash scripts/typecheck.sh",
    "typecheck:full": "tsc -b",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\" --ignore-path .prettierignore",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.6",
    "pako": "^2.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-mosaic-component": "^6.1.1",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
